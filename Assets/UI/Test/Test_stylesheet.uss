.label {
    opacity: 1;
    overflow: visible;
    visibility: visible;
    flex-grow: 0;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    -unity-font: url('project://database/Assets/Font/spaceagecyrillic_regular.ttf?fileID=12800000&guid=44df9d49d5267ea43a1a9684c348087d&type=3#spaceagecyrillic_regular');
    -unity-font-definition: initial;
    -unity-font-style: normal;
    font-size: 40px;
    -unity-text-align: upper-left;
    color: rgb(243, 100, 100);
    white-space: normal;
    -unity-text-outline-width: 2px;
    -unity-text-outline-color: rgb(0, 0, 0);
    text-shadow: 6.2px -4.8px 0 rgb(0, 0, 0);
    flex-basis: auto;
}

.labelContainer {
    flex-grow: 1;
    border-left-width: 5px;
    border-right-width: 5px;
    border-top-width: 5px;
    border-bottom-width: 5px;
    border-top-left-radius: 2px;
    border-bottom-left-radius: 2px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    justify-content: center;
    align-items: stretch;
    flex-wrap: nowrap;
    flex-direction: column;
    overflow: visible;
    background-color: rgba(0, 0, 0, 255);
}

.labelContainer:hover {
    background-color: rgba(56, 56, 56, 255);
}
